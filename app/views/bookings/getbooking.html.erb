<h2>List of booking for Show</h2>
<table>
<th>ID</th>
<th>show_id</th>
<th>seat</th>
<th>user_id</th>

<% @bookings.each do |booking| %>
  <tr>
    <td><%= booking.id %></td>
    <td><%= booking.show_id %></td>
    <td><%= booking.seat %></td>
    <td><%= booking.user_id %></td>
  </tr>
<% end %>
</table>
